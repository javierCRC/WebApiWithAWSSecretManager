{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Config": {
    "OriginCors": "http://localhost:55917,http://localhost:4000,https://www.google.com"
  },
  "CustomSwaggerUi": {
    "Personalised": true, // If you want to use a standard swagger.ui change to false
    "HeaderImg": "<img src='https://notitalamanca.blob.core.windows.net/allimages/header.svg' style='display:block; margin: 0px auto;' alt='not supported'>",
    "DocTitle": "Swagger UI Personalized",
    "PathCss": "/css/CustomSwaggerStyles.css"
  },
  "AwsSecretsManagers": {
    "Manager1": {
      "SecretManagerName": "ElPillazo-Api-Marketing-ASM",
      "AccessKey": "your-access-key-1",
      "SecretKey": "your-secret-key-1",
      "Region": "us-east-1"
    },
    "Manager2": {
      "SecretManagerName": "your-secret-manager-name-2",
      "AccessKey": "your-access-key-2",
      "SecretKey": "your-secret-key-2",
      "Region": "eu-west-1"
    }
  },
  "AllowedHosts": "*"
}
